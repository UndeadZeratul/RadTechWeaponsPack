// This adds it to options so we don't have multiple option menus in the main options menu.
AddOptionMenu "OptionsMenu"
{
    Submenu "$RADTECH_MENU", "RadTechWeaponry"
}

// Same as above but HDAddonMenu
AddOptionMenu "HDAddonMenu"
{
    Submenu "$RADTECH_MENU", "RadTechWeaponry"
}


OptionMenu FlareGuns
{
	Title "$MENU_FGUNTITLE"
   	StaticText "$MENU_NOTICE", "Yellow"
    StaticText  "$MENU_SUPPRESS1", "white"
    StaticText  "$MENU_SUPPRESS2", "white"
   	StaticText ""
	StaticText "$MENU_SPAWNOPTIONS", "Yellow"
    StaticText  "$MENU_FGUNPLASTSPAWNTEXT", "white"
	Slider "$MENU_WEPSPAWNRATE", "fl_weapon_spawn_bias", 0.0, 100, .01, 2
	SafeCommand "$MENU_RESETWEP", "resetcvar fl_weapon_spawn_bias"
	StaticText ""
	StaticText  "$MENU_PFGUNDROPENEMY", "white"
	Slider "$MENU_WEPDROPRATE", "fl_weapon_drop_chance_bias", 0.0, 100, .01, 2
	SafeCommand "$MENU_RESETDROP", "resetcvar fl_weapon_drop_chance_bias"
	StaticText ""
	StaticText  "$MENU_PFGUNBACKPACK", "white"
	Option "$MENU_BACKPACKSALLOWED", "fl_weapon_allowBackpacks", "OnOff"
	StaticText ""
    StaticText  "$MENU_PFGUNPERSISTENCY", "white"
	Option "$MENU_PERSISTENCY", "fl_weapon_persistent_spawning", "OnOff"
	SafeCommand "$MENU_RESETPERSISTENCY", "resetcvar fl_weapon_persistent_spawning"
 	StaticText ""
    /* StaticText  "$MENU_MFGNCONVERCONTROL", "white"
	Option "$MENU_METALCONVRATE", "fl_metal_chance_bias", "DropBias"
	SafeCommand "$MENU_RESETCONV", "resetcvar fl_metal_chance_bias"
	StaticText "" */
	StaticText  "$MENU_MFGUNDROP", "white"
	Slider "$MENU_WEPDROPRATE", "fl_metal_drop_chance_bias", 0.0, 100, .01, 2
	SafeCommand "$MENU_RESETDROP", "resetcvar fl_metal_drop_chance_bias"
	StaticText ""
	StaticText  "$MENU_MFGUNBACKPACK", "white"
	Option "$MENU_BACKPACKSALLOWED", "fl_metal_allowBackpacks", "OnOff"
	StaticText ""
    StaticText  "$MENU_MFGUNPERSISTENCY", "white"
	Option "$MENU_PERSISTENCY", "fl_metal_persistent_spawning", "OnOff"
	SafeCommand "$MENU_PERSISTENCY", "resetcvar fl_metal_persistent_spawning"
 	StaticText ""
   	StaticText ""
	StaticText "$MENU_RESETOPTIONS", "Yellow"
	SafeCommand "$MENU_RESETALLOPTIONS", "flreset_cvars"
	StaticText ""
	StaticText "$MENU_LORETAG", "Yellow"
	StaticText ""
	StaticText "$MENU_FGUNLORE1", "white"
	StaticText "$MENU_FGUNLORE2", "white"
	StaticText "$MENU_FGUNLORE3", "white"
}

